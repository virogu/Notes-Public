<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IpEditorText">
        <attr name="ipText" format="string" />
        <attr name="itemWidth" format="dimension" />
        <attr name="itemCount" format="integer" />
        <attr name="itemHeight" format="dimension" />
        <!--  define which items is not editable, multi index split with "," , such as "0,1,2" -->
        <attr name="itemDisableIndex" format="string" />
        <attr name="textSize" format="dimension" />
        <attr name="splitText" format="string" />
        <attr name="textColor" format="color" />
        <attr name="textDisableColor" format="color" />
        <attr name="layoutStyle">
            <enum name="one_piece" value="0" />
            <enum name="separate" value="1" />
        </attr>
        <attr name="boxStrokeColor" format="color" />
        <attr name="boxStrokeWidth" format="dimension" />
        <attr name="boxBackgroundColor" format="color" />
        <attr name="boxCornerRadius" format="dimension" />
        <attr name="boxCornerRadiusTopStart" format="dimension" />
        <attr name="boxCornerRadiusTopEnd" format="dimension" />
        <attr name="boxCornerRadiusBottomStart" format="dimension" />
        <attr name="boxCornerRadiusBottomEnd" format="dimension" />
    </declare-styleable>
</resources>